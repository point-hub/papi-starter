{
  "name": "@point-hub/papi-starter",
  "version": "0.0.1",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development bun run --watch src/index.ts",
    "start": "cross-env NODE_ENV=production bun run src/index.ts",
    "types": "tsc -p tsconfig.json",
    "test": "bun test",
    "test:junit": "bun -e \"require('fs').mkdirSync('test-reports', { recursive: true })\" && bun test --reporter=junit --reporter-outfile=test-reports/test-report.xml",
    "test:pdf": "bun run scripts/test-report-pdf.ts",
    "test:txt": "bun run scripts/test-report-txt.ts",
    "test:report": "bun run test:junit; bun run test:pdf; bun run test:txt",
    "test:coverage": "bun test --coverage",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@types/bun": "^1.3.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/supertest": "^6.0.3",
    "@types/validatorjs": "^3.15.5",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "9.39.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "pdfkit": "^0.17.2",
    "supertest": "^7.2.2",
    "xml2js": "^0.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@point-hub/express-utils": "0.2.12",
    "@point-hub/papi": "^0.5.55",
    "ably": "^2.17.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "qs": "^6.14.1",
    "ua-parser-js": "^2.0.8",
    "validatorjs": "^3.22.1"
  }
}